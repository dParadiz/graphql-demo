{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.1.3",
    "ext-json": "*",
    "mongodb/mongodb": "^1.4",
    "psr/container": "^1.0",
    "psr/http-message": "^1.0",
    "psr/http-server-handler": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "psr/log": "^1.0",
    "webonyx/graphql-php": "^0.12.4",
    "zendframework/zend-diactoros": "^1.8",
    "zendframework/zend-httphandlerrunner": "^1.0",
    "ext-mongodb": "*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "auto-scripts": {

    }
  }
}
